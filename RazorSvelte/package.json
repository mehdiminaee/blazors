{
  "name": "svelterazoraspnet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dotnet-run": "dotnet run",
    "dotnet-clean": "dotnet clean",
    "dotnet-build": "dotnet build",
    "scss-build": "node-sass ./Styles/style.scss ./wwwroot/style.css --output-style compressed",
    "scss-watch": "node-sass ./Styles/style.scss ./wwwroot/style.css --output-style compressed --watch --recursive",
    "index-build": "rollup -c ./Pages/Index.cshtml.js",
    "index-watch": "rollup -c ./Pages/Index.cshtml.js -w",
    "build-all-pages": "node ./Scripts/build-all.js",
    "upgrade-npms": "ncu --upgrade && npm install"
  },
  "devDependencies": {
    "@popperjs/core": "^2.9.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jquery": "^3.5.6",
    "bootstrap": "^5.1.0",
    "jquery": "^3.6.0",
    "node-sass": "^6.0.1",
    "rollup": "^2.56.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.37.5",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "svelte-spa-router": "^3.2.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
