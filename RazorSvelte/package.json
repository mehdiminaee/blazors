{
  "name": "svelterazoraspnet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dotnet-run": "dotnet run",
    "dotnet-clean": "dotnet clean",
    "dotnet-build": "dotnet build",
    "scss-build": "node-sass ./Styles/style.scss ./wwwroot/style.css --output-style compressed",
    "scss-watch": "node-sass ./Styles/style.scss ./wwwroot/style.css --output-style compressed --watch --recursive",
    "index-build": "rollup -c ./Pages/Index.rollup.config.js",
    "index-watch": "rollup -c ./Pages/Index.rollup.config.js -w",
    "build-all": "node ./Scripts/build-all.js",
    "upgrade-npms": "ncu --upgrade && npm install && npm rebuild node-sass && npm audit fix",
    "audit-fix": "npm audit fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "bootstrap": "^5.1.1",
    "node-sass": "^6.0.1",
    "rollup": "^2.57.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.42.1",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.5",
    "svelte-spa-router": "^3.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
